      "metadata": {},
      "outputs": [],
      "source": [
        "# Example 6: Exception Handling\n",
        "try:\n",
        "    result = 10 / 0\n",
        "except ZeroDivisionError:\n",
        "    print(\"Error: Division by zero is not allowed!\")\n",
        "finally:\n",
        "    print(\"Execution completed.\")"
      ]
    },
