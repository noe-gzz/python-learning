      "source": [
        "# Example 4: NumPy Operations\n",
        "import numpy as np\n",
        "\n",
        "# Creating arrays\n",
        "array = np.array([1, 2, 3, 4, 5])\n",
        "print(\"Original Array:\", array)\n",
        "\n",
        "# Array operations\n",
        "print(\"Mean:\", np.mean(array))\n",
        "print(\"Sum:\", np.sum(array))"
